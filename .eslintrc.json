{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": [2, "always"],
    "no-dupe-args": 2,
    "no-extra-semi": 2,
    "eqeqeq": 2,
    "no-invalid-this": 2,
    "no-multi-spaces": 2,
    "no-unused-vars": 1,
    "eol-last": 2,
    "max-lines": [1, 3500, {"skipBlankLines": true, "skipComments": true}],
    "max-params": [1, 10],
    "new-parens": [2, "always"],
    "semi-style": [2, "last"],
    "no-var": 2,
    "quotes": [2, "single"],
    "no-multiple-empty-lines": [2, {"max": 3}],
    "strict": 2,
    "indent": [2, 2],
    "no-await-in-loop": 1,
    "no-empty": [2, {"allowEmptyCatch": true}],
    "max-classes-per-file": [2, 3],
    "yoda": 2,
    "comma-style": [1, "last"],
    "func-call-spacing": [1, "never"],
    "jsx-quotes": [1, "prefer-double"],
    "no-tabs": [2, {"allowIndentationTabs": true}],
    "constructor-super": 1,
    "arrow-spacing": [1, {"before": true, "after": true}],
    "brace-style": 2
  }
}
